{
  "outputDirectory": "public",
  "buildCommand": "npm run build && mkdir -p public",
  "functions": {
    "api/cron.js": {
      "maxDuration": 60
    },
    "api/health.js": {
      "maxDuration": 10
    }
  },
  "crons": [
    {
      "path": "/api/cron",
      "schedule": "* * * * *"
    }
  ]
}